    ns1:positive_sentiment 1.364897e+01 .

ns1:2.8 a ns1:fragments ;
    ns1:containsText """ Men are not easily seen to be brought into evil case by failure to consider
what passes in another’s soul; but they who do not read aright the motions
of their own soul are bound to be in evil case.
""" ;
    ns1:hasAssociatedConcept <https://w3id.org/arco/ontology/core/soul/> ;
    ns1:hasAssociatedFigure <https://w3id.org/arco/ontology/core/AbstractFigure/man> ;
